         <Label alignment="CENTER" prefHeight="17.0" prefWidth="113.0" text="Happiness" textFill="#ffae00">
            <font>
               <Font size="20.0"/>
            </font>
         </Label>
         <Label fx:id="lblHappinessStat" alignment="CENTER" prefHeight="17.0" prefWidth="107.0" text="stat">
            <font>
               <Font size="32.0"/>
