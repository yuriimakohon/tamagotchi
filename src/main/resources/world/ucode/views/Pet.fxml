            <font>
               <Font size="32.0"/>
            </font>
         </Label>
         <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#petClean"
                 prefHeight="25.0" prefWidth="56.0" text="Clean"/>
