<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="640.0" prefWidth="420.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="world.ucode.control.CreateMenuController">
   <children>
      <TextField fx:id="tfieldPetName" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="37.0" prefWidth="218.0" promptText="Yours new pets name">
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
         <font>
            <Font size="16.0" />
         </font></TextField>
      <RadioButton fx:id="rbuttonCat" mnemonicParsing="false" selected="true" text="Cat">
         <font>
            <Font size="16.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="togglegroupPetType" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rbuttonDog" mnemonicParsing="false" text="Dog" toggleGroup="$togglegroupPetType">
         <font>
            <Font size="16.0" />
         </font>
      </RadioButton>
      <Label text="Max Health">
         <VBox.margin>
            <Insets top="40.0" />
         </VBox.margin>
         <font>
            <Font size="18.0" />
         </font></Label>
      <Slider fx:id="sliderMaxHealth" blockIncrement="1.0" majorTickUnit="1.0" max="8.0" minorTickCount="0" prefHeight="24.0" prefWidth="233.0" showTickMarks="true" snapToTicks="true" />
      <Label fx:id="lblMaxHealthValue" text="200" textFill="#b50000">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Label>
      <Button fx:id="btnCreate" disable="true" mnemonicParsing="false" onAction="#createNewPet" prefHeight="42.0" prefWidth="114.0" text="Create">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
         <font>
            <Font size="16.0" />
         </font></Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets top="100.0" />
   </padding>
</VBox>
